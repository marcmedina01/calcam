{
  "name": "CalorieCam",
  "version": "3.0.0",
  "description": "Calorie Cam",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "cd react-ui && npm start",
    "build": "cd react-ui/ && npm install && npm run build",
    "start-ui": "cd react-ui && npm start",
    "start-local": "concurrently --kill-others \"npm run start-ui\""
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^1.19.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "nodemon": "^2.0.14",
    "openai": "^4.47.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT"
}
